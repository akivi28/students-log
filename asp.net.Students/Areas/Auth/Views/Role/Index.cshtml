@model List<Microsoft.AspNetCore.Identity.IdentityRole<int>>

<div id="rolesTable">
    <table class="table table-striped">
        <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var role in Model)
        {
            <tr>
                <td>@role.Id</td>
                <td>@role.Name</td>
            </tr>
        }
        </tbody>
    </table>
</div>
